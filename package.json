{
  "name": "awsprofile",
  "description": "CLI to manage AWS profiles and renew STS tokens",
  "version": "2.1.1",
  "author": "Prasad Domala",
  "bin": {
    "awsprofile": "./bin/run"
  },
  "bugs": "https://github.com/kheriox-technologies/awsprofile/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@types/ini": "^1.3.30",
    "@types/inquirer": "^7.3.1",
    "aws-sdk": "^2.868.0",
    "boxen": "^5.0.0",
    "chalk": "^4.1.0",
    "cli-table3": "^0.6.0",
    "clipboardy": "^2.3.0",
    "command-exists": "^1.2.9",
    "fs-extra": "^9.1.0",
    "ini": "^2.0.0",
    "inquirer": "^8.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ora": "^5.4.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/command-exists": "^1.2.0",
    "@types/fs-extra": "^9.0.10",
    "@types/mocha": "^5",
    "@types/node": "^14.14.37",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kheriox-technologies/awsprofile",
  "keywords": [
    "aws",
    "aws profile",
    "mfa",
    "sts",
    "aws cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "awsprofile",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "hooks": {
      "init": "./lib/hooks/init/init"
    }
  },
  "repository": "kheriox-technologies/awsprofile",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
